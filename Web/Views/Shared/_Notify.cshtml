@*/////////////////////////////////////////////////////////*@
@if (TempData["OK"] != null)
{
      <div class="alert alert-dismissible alert-success">
      <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
       @TempData["OK"]
    </div>

        
}
else if (TempData["NO"] != null)
{
           <div class="alert alert-dismissible alert-danger">
      <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
      @TempData["NO"]
    </div>

           
}

@*/////////////////////////////////////////////////////////////////////////////////*@